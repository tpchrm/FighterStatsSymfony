{% extends 'base.html.twig' %}

{% block title %}Hello DivisionMenController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <table>
        <thead>
        <tr>
            <th>Francais</th>
            <th>Anglais</th>
        </tr>
        </thead>
        <tbody>
        {% for division in divisions_mens %}
            <tr>
                <td>{{ division.divisionFr }}</td>
                <td>{{ division.divisionEng }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>
{% endblock %}
